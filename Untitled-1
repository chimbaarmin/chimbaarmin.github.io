    jQuery(document).ready(function($){
        
        
        /* BOF SUBSCRIPTION */
        $('input[name="comment_meta[subscr_value]"]').change(function(){
            if($(this).is(":checked")) {
                $('input[name="comment_meta[subscr_email]"]').slideDown(function(){
                    $(this).focus();
                })
            } else {
                $('input[name="comment_meta[subscr_email]"]').slideUp();
            }
        });
        
        $('input[name="comment_meta[subscr_email]"]').on('change', function(){
            //checkSubmitActionAvailability();
            return false;
        });
        /* EOF SUBSCRIPTION */
    });
